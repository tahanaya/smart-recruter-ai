    <!-- Profils de Postes de Démonstration -->
    <record id="job_profile_python_dev" model="smart.recruiter.job.profile">
        <field name="name">Développeur Python Senior</field>
        <field name="description">Développeur Python avec expertise Odoo et frameworks web modernes</field>
        <field name="priority">high</field>
        <field name="min_score_threshold">70</field>
        <field name="active">True</field>
        <field name="color">3</field>
    </record>

    <record id="job_profile_fullstack" model="smart.recruiter.job.profile">
